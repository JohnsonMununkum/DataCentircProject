<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Grades Page</title>
</head>
<body>
    <h1>Grades</h1>
    <br>

     <!-- link to home page -->
    <a href="/">Home</a>

        <!-- Display all Students grades in a table name, module, grade  -->
        <table border="1">
            <thead>
                <tr>
                    <th>Student</th>
                    <th>Module</th>
                    <th>Grade</th>
                </tr>
            </thead>
            <tbody>
                <% grades.forEach(grade => { %>
                    <tr>
                        <td><%= grade.name %></td>
                        <td><%= grade.mid %></td>
                        <td><%= grade.grade %></td>
                        <td>
                        </td>
                    </tr>
                <% }); %>
            </tbody>
        </table>
</body>
</html>
